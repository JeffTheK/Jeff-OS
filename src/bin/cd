#!python

import os
import sys

def main():
    if len(sys.argv) != 3:
        print("wrong number of arguments")
        return
    
    path = sys.argv[2] 
    if os.path.isdir(path) or path == "../":
        os.chdir(path)
    else:
        print("wrong path")
        return

if __name__ == "__main__":
    main()