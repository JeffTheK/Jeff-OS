#!/usr/bin/env python3

import os
import sys

def main():
    if len(sys.argv) != 3:
        print("wrong number of arguments")
        return
    path = sys.argv[2]
    if not os.path.isfile(path):
        print("file not found")
        return

    file = open(path, 'r')
    text = file.read()
    print(text)

if __name__ == "__main__":
    main()